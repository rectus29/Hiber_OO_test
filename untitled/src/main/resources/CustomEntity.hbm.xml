<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.edeal.frontline.entities.sys">
    <union-subclass name="CustomEntity" extends="GenericEntity" abstract="true">
        <map name="customFieldMap" cascade="all" >
            <key column="name"/>
            <many-to-many class="CustomField" column="genericEntityiId"/>
        </map>
    </union-subclass>
</hibernate-mapping>